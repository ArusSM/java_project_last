<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="150.0" prefWidth="150.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="views.BlockController">
	<children>
		<ImageView fx:id="img" fitHeight="100.0" fitWidth="150.0"
			pickOnBounds="true" />
		<Button fx:id="nobtn" layoutY="100.0" mnemonicParsing="false"
			onAction="#noBtnClick" prefHeight="50.0" prefWidth="75.0" text="노말" />
		<Button fx:id="exbtn" layoutX="75.0" layoutY="100.0"
			mnemonicParsing="false" onAction="#exBtnClick" prefHeight="50.0"
			prefWidth="75.0" text="전문가" />
	</children>
</AnchorPane>
